{
  "name": "@astrape/plugin",
  "version": "0.1.0",
  "description": "Native Rust-powered multi-agent orchestration for Claude Code",
  "type": "module",
  "main": "native/index.js",
  "types": "native/index.d.ts",
  "exports": {
    ".": {
      "import": "./native/index.js",
      "types": "./native/index.d.ts"
    }
  },
  "files": [
    "native",
    "agents",
    "hooks",
    "commands",
    ".claude-plugin",
    "CLAUDE.md"
  ],
  "scripts": {
    "dev:link": "node scripts/dev-link.cjs",
    "dev:unlink": "node scripts/dev-unlink.cjs",
    "build": "bun run build:native && bun run copy:native",
    "build:native": "cd ../crates/astrape-napi && bun run build",
    "copy:native": "cp ../crates/astrape-napi/index.js ../crates/astrape-napi/index.d.ts ../crates/astrape-napi/*.node native/",
    "verify": "node ../crates/astrape-napi/scripts/verify-build.js",
    "test:hooks": "echo '{\"prompt\": \"ultrawork test\"}' | bun hooks/user-prompt-submit.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/junhoyeo/astrape"
  },
  "keywords": [
    "claude",
    "claude-code",
    "multi-agent",
    "orchestration",
    "rust",
    "napi",
    "native"
  ],
  "author": "Junho Yeo <i@junho.io>",
  "license": "MIT"
}
